{
  "expo": {
    "name": "realm_rn",
    "slug": "realm_rn",
    "sdkVersion": "47.0.0",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/icon.png",
    "scheme": "realm",
    "splash": {
      "image": "./src/assets/images/splash.png",
      "resizeMode": "contain"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "platforms": [
      "ios"
    ],
    "ios": {
      "infoPlist": {
        "NSAppTransportSecurity": {
          "NSAllowArbitraryLoads": true
        }
      },
      "bundleIdentifier": "com.realmpartyapp.realm-rn"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with other users."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "a7edb0ed-e973-4ad7-8f55-90b953505514"
      }
    }
  }
}
