{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }, 
  "react-native": {
    "analytics_auto_collection_enabled": true,
    "google_analytics_automatic_screen_reporting_enabled": true
  }
}
